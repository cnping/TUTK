                              io_TxBuf[7] = pBSS->InfrastructureMode;
                              io_TxBuf[8] = pBSS->Config.Channel;
                              io_TxBuf[9] = pBSS->Rssi;
                              io_TxBuf[10] = pBSS->EncryptMode;
                              io_TxBuf[11] = IsTheLast;
                              ptr = &io_TxBuf[12];
